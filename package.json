{
  "name": "@zenika/sensei",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "npm install --prefix=src/pdf",
    "prettier": "prettier \"{*,**/*}.{yml,html,css,js}\"",
    "prettier:check": "npm run prettier -- --check",
    "prettier:write": "npm run prettier -- --write",
    "test": "node test.js",
    "test:watch": "nodemon test.js --ignore dist",
    "start": "node bin/sensei.js"
  },
  "bin": {
    "sensei": "bin/sensei.js"
  },
  "dependencies": {
    "css-loader": "4.2.0",
    "expose-loader": "2.0.0",
    "extract-loader": "5.1.0",
    "file-loader": "6.2.0",
    "html-loader": "2.1.2",
    "html-webpack-plugin": "5.3.1",
    "markdown-loader": "6.0.0",
    "marked": "2.0.0",
    "mini-css-extract-plugin": "1.5.1",
    "prismjs": "1.23.0",
    "reveal.js": "4.0.2",
    "webpack": "5.36.0",
    "webpack-cli": "4.6.0",
    "webpack-dev-server": "3.11.2",
    "yargs": "16.2.0"
  },
  "devDependencies": {
    "nodemon": "2.0.7",
    "prettier": "2.0.5"
  }
}

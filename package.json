{
  "name": "@zenika/sensei",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "npm install --prefix=src/pdf",
    "prettier": "prettier \"{*,**/*}.{yml,html,css,js}\"",
    "prettier:check": "npm run prettier -- --check",
    "prettier:write": "npm run prettier -- --write",
    "test": "tap \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "start": "webpack-dev-server"
  },
  "bin": {
    "sensei": "bin/sensei.js"
  },
  "dependencies": {
    "css-loader": "4.2.0",
    "expose-loader": "1.0.0",
    "file-loader": "6.0.0",
    "html-loader": "1.1.0",
    "html-webpack-plugin": "4.3.0",
    "markdown-loader": "6.0.0",
    "marked": "1.2.7",
    "mini-css-extract-plugin": "0.9.0",
    "prismjs": "1.21.0",
    "reveal.js": "4.0.2",
    "webpack": "4.44.1",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  },
  "devDependencies": {
    "prettier": "2.0.5",
    "tap": "14.10.7"
  }
}
